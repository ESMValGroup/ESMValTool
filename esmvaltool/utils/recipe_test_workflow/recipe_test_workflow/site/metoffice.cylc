#!jinja2
[runtime]
    [[COMPUTE]]
        platform = spice
        [[[directives]]]
            --wckey = RTW
            --ntasks = {{ MAX_PARALLEL_TASKS }}

    [[build_container]]
        execution time limit = PT30M
        [[[directives]]]
            --mem = 2G

    [[process<fast>]]
        execution time limit = PT2M
        [[[directives]]]
            --mem = 2G

    [[process<medium>]]
        execution time limit = PT6M
        [[[directives]]]
            --mem = 2G
