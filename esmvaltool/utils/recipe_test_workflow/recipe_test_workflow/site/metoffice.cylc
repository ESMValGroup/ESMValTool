#!jinja2
[runtime]
    [[COMPUTE]]
        platform = spice
        [[[directives]]]
            --wckey = RTW
            --ntasks = {{ MAX_PARALLEL_TASKS }}

    [[process<fast>]]
        [[[directives]]]
            --time = 2
            --mem = 2G

    [[process<medium>]]
        [[[directives]]]
            --time = 6
            --mem = 2G
