#!jinja2
[runtime]
    [[COMPUTE]]
        platform = spice
        [[[directives]]]
            --wckey = RTW
            --ntasks = {{ MAX_PARALLEL_TASKS }}

    [[process<small>]]
        [[[directives]]]
            --time = 4
            --mem = 6G

    [[process<medium>]]
        [[[directives]]]
            --time = 8
            --mem = 6G
