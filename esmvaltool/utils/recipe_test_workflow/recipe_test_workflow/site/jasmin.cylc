#!jinja2
[runtime]
    [[COMPUTE]]
        platform = lotus
        [[[directives]]]
            --wckey = RTW
            --ntasks = 1

    [[process<fast>]]
        [[[directives]]]
            --time = 2
            --mem = 2G

    [[process<medium>]]
        [[[directives]]]
            --time = 6
            --mem = 2G
