        <diag_script cfg="cfg_perfmetrics_grading_RMSD_all_glob.ncl">  perfmetrics_grading.ncl </diag_script>
        <diag_script cfg="cfg_perfmetrics_grading_RMSD_all_glob.ncl">  perfmetrics_taylor.ncl </diag_script>
    </diag>

    <diag>
